{
    "manifest_version": 2,
    "version": "1.4.dev.7",
    "name": "hikingmaps",
    "title": "HikingMaps",
    "description": "Map application for outdoor enthusiasts",
    "framework": "ubuntu-sdk-14.04-html",
    "app": {
	"launch": {
	    "local_path": "/www/index.html"
	}
    },
    "hooks": {
        "hikingmaps": {
            "apparmor": "hikingmaps.json",
            "desktop": "hikingmaps.desktop"
        }
    },
    "icons":
    {
	"60": "/hikingmaps60.png",
	"128": "/hikingmaps128.png",
	"512": "/hikingmaps512.png"
    },
    "maintainer": "Christof Meerwald <cmeerw@cmeerw.org>",
    "developer":
    {
	"name": "Christof Meerwald",
	"url": "http://cmeerw.org"
    },
    "permissions":
    [
	"geolocation"
    ]
}
